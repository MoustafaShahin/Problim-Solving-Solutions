#include <iostream>
#include <math.h>
#include <iomanip>
//problem link
//https://a2oj.com/p?ID=347
using namespace std;
int a,b,c;
double findx()
{
    double root = -b+sqrt(pow(b,2)-4*(a*c));
    return root / (2*a) ;
}
int main()
{
    std::cout << std::fixed;
    std::cout << std::setprecision(6);
    cin>>a>>b>>c;
    cout<<findx()<<endl;
    return 0;
}
