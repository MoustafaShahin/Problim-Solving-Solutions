import java.math.BigInteger;
import java.util.Scanner;
// big mod
//https://uva.onlinejudge.org/index.php?option=com_onlinejudge&Itemid=8&page=show_problem&problem=310
class Main {
    public static void main(String args[]) {
        BigInteger B, P, M;
        Scanner s = new Scanner(System.in);
        while(s.hasNext()) {
            B = s.nextBigInteger();
            P = s.nextBigInteger();
            M = s.nextBigInteger();

            System.out.println(B.modPow(P, M).toString());
        }
    }
}
